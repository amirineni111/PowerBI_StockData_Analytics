        "Y": {
          "projections": [
            {
              "field": {
                "Measure": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "_Measures"
                    }
                  },
                  "Property": "AI Total Predictions"
                }
              },
              "queryRef": "_Measures.AI Total Predictions",
              "nativeQueryRef": "AI Total Predictions"
            },
            {
              "field": {
                "Measure": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "_Measures"
                    }
                  },
                  "Property": "AI Correct Predictions"
                }
              },
              "queryRef": "_Measures.AI Correct Predictions",
              "nativeQueryRef": "AI Correct Predictions"
            }
          ]
        }
      }
    },
    "objects": {
      "legend": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
