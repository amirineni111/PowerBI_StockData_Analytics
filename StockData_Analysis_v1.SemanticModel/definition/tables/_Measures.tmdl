	measure 'AI Direction Accuracy %' =
			VAR CorrectCount = COUNTROWS(FILTER(ai_prediction_history, [direction_correct] = TRUE))
			VAR TotalCount = COUNTROWS(FILTER(ai_prediction_history, NOT(ISBLANK([direction_correct]))))
			RETURN DIVIDE(CorrectCount, TotalCount, 0)
		formatString: 0.00%;-0.00%;0.00%
		displayFolder: AI Predictions
		lineageTag: m001-ai-direction-accuracy

	measure 'AI MAPE %' = AVERAGE(ai_prediction_history[percentage_error])
