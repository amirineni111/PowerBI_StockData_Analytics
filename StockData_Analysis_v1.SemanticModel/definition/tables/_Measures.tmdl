	partition _Measures = m
		mode: import
		source =
				let
				    Source = Table.FromRows(Json.Document(Binary.Decompress(Binary.FromText("i44FAA==", BinaryEncoding.Base64), Compression.Deflate)), let _t = ((type text) meta [Serialized.Text = true]) in type table [tmp = _t]),
				    #"Removed Columns" = Table.RemoveColumns(Source,{"tmp"})
				in
				    #"Removed Columns"

	annotation PBI_ResultType = Table

