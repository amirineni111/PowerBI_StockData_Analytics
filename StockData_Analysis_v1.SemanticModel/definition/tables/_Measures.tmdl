	measure 'Value Score' =
			VAR AvgPE = [NSE Avg PE]
			VAR PE_Score = IF(AvgPE < 15, 50, IF(AvgPE < 25, 30, 0))
			VAR AvgPB = [NSE Avg PB]
			VAR PB_Score = IF(AvgPB < 1.5, 50, IF(AvgPB < 3, 30, 0))
			RETURN PE_Score + PB_Score
		formatString: 0
		displayFolder: Fundamental Scores
		lineageTag: m120-nse-value-score

	measure 'Quality Score' =
			VAR AvgROE = [NSE Avg ROE]
			VAR ROE_Score = IF(AvgROE > 0.20, 100, IF(AvgROE > 0.10, 60, 0))
			RETURN ROE_Score
		formatString: 0
		displayFolder: Fundamental Scores
		lineageTag: m121-nse-quality-score

